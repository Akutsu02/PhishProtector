{

	"manifest_version": 2,
	"name": "PhishProtector",
	"version": "1.0",
	"permissions": [
		"<all_urls>",
		"webRequest",
		"webRequestBlocking",
		"webNavigation",
		"tabs",
		"activeTab"
	],
	"browser_action": {
		"default_icon": {
			"48": "Icons/ok.png"
		},
		"default_popup": "popup.html"
	},
	"description": "Protection contre le phishing",

	"icons": {
		"48": "Icons/Icon.png"
	},
 
	"background": {
		"scripts": [ "background.js" ]
	}

}